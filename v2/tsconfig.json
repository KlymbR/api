{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es5",
      "es6",
      "es2015"
    ],
    "types": [
      "node"
    ],
    "sourceMap": true,
    "outDir": "./build",
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}